  .ic-Layout-wrapper, .ic-Layout-contentMain, #wiki_show_view_main { overflow: visible !important; }
  .ic-Layout-wrapper, .ic-Layout-columns, .ic-app-main-content { display: block !important; }
  .ic-Layout-contentMain { width: auto; padding: 0px !important; }
  .ic-Layout-contentMain .fill_height_div { overflow: visible !important; }
  a:link, a:visited { font-weight: bold; text-decoration: underline; }
  a:link::after, a:visited::after { content: " (" attr(href) ") "; font-size: 90%; }
